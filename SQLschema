// add name of event
TABLE events = {
    COLUMN event_id int
    COLUMN tutor_id int
    COLUMN time (date object)
  //  COLUMN event_name varchar
}

TABLE event_topics = {
    COLUMN event_id int
    COLUMN topic_id int
}

TABLE available_topics = {
    COLUMN topic_id int
    COLUMN topic varchar
}

// added image_url and tutor_linkedin
TABLE tutors = {
    COLUMN tutor_id int
    COLUMN name varchar
 //   COLUMN image_url varchar
 //   COLUMN tutor_linkedin varchar
}


TABLE tutor_topics = {
    COLUMN tutor_id int
    COLUMN topic_id int
}

// added student's image_url and student_linkedin
TABLE students = {
    COLUMN student_id int
    COLUMN name varchar
 //   COLUMN image_url varchar
 //   COLUMN student_linkedin varchar
}

TABLE student_events = {
    COLUMN event_id int
    COLUMN student_id int
}

TABLE upvotes = {
    COLUMN event_id int
    COLUMN topic_id int
    COLUMN upvotes int
}

TABLE event_messages = {
    COLUMN event_id int
    COLUMN student_id int // either student or tutor ID required
    COLUMN tutor_id int // either student or tutor ID required
    COLUMN message varchar
}

